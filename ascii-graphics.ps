%!PS
% Print images compatible with the 'format' in
% Bob Neill's Book of Typewriter Art
% Here: Kojak
% Page 84-90
% https://pbs.twimg.com/media/DrgVJYfXQAAgZ2f.jpg
% (that image is not complete, will have to look at full book)
%%

/start-y 700 def
/fontsize 12 def
/linefeed fontsize 0.55 mul def

/Courier-Bold findfont fontsize scalefont setfont

% count character p -
/p {
    2 dict begin
    /ch exch def
    dup
    string /str exch def
    1 sub 0 1 3 -1 roll {
	str exch ch put
    } for
    str show
    end
} def

% typewrite the instructions for given line.
% line (instructions) tw -
/tw {
    3 dict begin
    exch linefeed mul start-y exch sub 10 exch moveto
    ( )   % search character
    {
	search
	/has-more exch def
	/instr exch def
	instr length /len exch def
	instr len 1 sub get /ch exch def
	ch 112 eq {   % if last char was 'p'=112 we might have 'sp'
	    instr len 2 sub get 115 eq {  % second last was 's'=115
		/ch 32 def
		instr len 2 sub 32 put
	    } if
	} if
	instr len 1 sub 32 put
	instr cvi ch p
	has-more not { exit } if
    } loop
    end
} def

